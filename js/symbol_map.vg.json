{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 1200,
  "height": 700,
  "title": "Number of F1 Races in Each Country since 1950",
  "projection": {"type": "equalEarth"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/Manan-python/try1/main/js/ne_10m_admin_0_countries_ind.json?token=GHSAT0AAAAAABZFWPVYXTFQJQNJ646KEO5CY2CC6IA",
        "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/Manan-python/FIT3179W9HW/main/data/no%20of%20races%20location%20wise.csv"
      },
      "mark": {"type": "circle", "tooltip": {"content": "data"}},
      "encoding": {
        "longitude": {"field": "lng", "type": "quantitative"},
        "latitude": {"field": "lat", "type": "quantitative"},
        "size": {
          "field": "Count",
          "type": "quantitative",
          "title": "No of races",
          "scale": {"domain": [0,80]}
        }
      }
    }
  ]
}
