{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 1200,
  "height": 700,
  "title": "Number of F1 Races in Each Country since 1950",
  "projection": {"type": "equalEarth"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/Manan-python/try1/main/js/ne_10m_admin_0_countries_ind.topojson",
        "format": {"type": "topojson", "feature": "ne_10m_admin_0_countries_ind"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/Manan-python/try1/main/data/zomato.csv"
      },
      "mark": {"type": "circle", "tooltip": {"content": "data"}},
      "encoding": {
        "longitude": {"field": "Longitude", "type": "quantitative"},
        "latitude": {"field": "Latitude", "type": "quantitative"},
        "size": {
          "field": "Dining_Rating",
          "type": "quantitative",
          "title": "No of races"
          "scale": {"domain": [0,5]}
        }
      }
    }
  ]
}
